package day2javalearning;

public class CompTwoStr {
	public static void main(String[] args)
	{
		String string1 = new String("Helloworld");
        String string2 = new String("Practice");
        String string3 = new String("world");
        String string4 = new String("world");
        String string5 = new String("world");
        System.out.println("Comparing " + string1 + " and " + string2+ " : " + string1.equals(string2));
        System.out.println("Comparing " + string3 + " and " + string4+ " : " + string3.equals(string4));
        System.out.println("Comparing " + string4 + " and " + string5+ " : " + string4.equals(string5));
        System.out.println("Comparing " + string1 + " and " + string4 + " : " + string1.equals(string4));
    }
}

output:
Comparing Helloworld and Practice : false
Comparing world and world : true
Comparing world and world : true
Comparing Helloworld and world : false
