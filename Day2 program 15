package day2javalearning;

public class IterateEachCharStr {
	public static void main(String[] args)
	{
		String name = "orange";
		System.out.println("Characters in " + name + "are:");
		for(int i = 0; i < name.length(); i++)
		{
			char a = name.charAt(i);
		System.out.print(a + ",");
	}
}
}

output:
Characters in orange are:
o,r,a,n,g,e,
