package day2javalearning;

public class GetCharStr {
	public static char getCharFromString(String str, int index)
    {
        return str.charAt(index);
    }
    public static void main(String[] args)
    {
        String str = "hellopeople";
        int index = 6;
        char ch = getCharFromString(str, index);
  
        System.out.println("Character from " + str+ " at index " + index+ " is " + ch);
    }
}

output:
Character from hellopeople at index 6 is e
