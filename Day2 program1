package day2javalearning;

public class LengthStr {
	public static void main(String[] args)
			{
	        String s1="howareyou";  
	        String s2="all";  
	        System.out.println("string length is: "+s1.length());  
	        System.out.println("string length is: "+s2.length());
	        }
			}
			
      output:
      string length is: 9
      string length is: 3
